{
  "hosting": {
    "source": ".",
    "use": "@firebase/nextjs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "invoice-backend",
          "region": "europe-west4"
        }
      }
    ]
  },
  "experiments": {
    "webframeworks": true
  }
}
